    "description": "Componente de uso comum entre SDKs para integração a partir de aplicações PHP com as APIs de Marketplaces",
    "keywords": ["sdk"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gpupo\\CommonSdk\\": "src",
            "Gpupo\\Tests\\CommonSdk\\": "tests"
        }
    },
    "require": {
        "php": ">=5.4",
        "psr/log": "~1.0",
        "gpupo/cache": "~1.2",
        "gpupo/common": "~1.3",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "minimum-stability" : "stable"
}
